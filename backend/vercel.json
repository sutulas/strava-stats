{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "functions": {
    "main.py": {
      "maxDuration": 30
    },
  
    "api/**": {
        "excludeFiles": "{*.md,venv,__pycache__,**/__pycache__,old_services,htmlcov,test_*.py,**/test_*.py,*.test.py,**/*.test.py,*.log,*.tmp,*.temp,coverage,.coverage,setup.*,tox.ini,.pytest_cache,.mypy_cache,.ruff_cache}/**"
    }
  }
}